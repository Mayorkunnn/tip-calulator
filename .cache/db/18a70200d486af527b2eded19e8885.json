d-color: var(--color-input);\r\n  color: var(--color-buttonandtext);\r\n  font-weight: 600;\r\n  text-align: right;\r\n}\r\n\r\n.input__container--custom::placeholder {\r\n  color: var(--color-label);\r\n  text-align: center;\r\n  font-size: 1rem;\r\n}\r\n\r\n.input__container--custom:focus {\r\n  border: 1.5px solid var(--color-reset);\r\n  border-radius: 5px;\r\n}\r\n\r\nlabel {\r\n  position: absolute;\r\n  left: 0;\r\n  top: -1rem;\r\n  font-size: 0.6rem;\r\n  font-weight: 700;\r\n}\r\n\r\n.input {\r\n  width: 100%;\r\n  padding: 0.5rem;\r\n  border: 0;\r\n  border-radius: 5px;\r\n  outline: none;\r\n  background-color: var(--color-input);\r\n  color: var(--color-buttonandtext);\r\n  font-weight: 600;\r\n  font-family: 'Space Mono', monospace;\r\n  transition: all 0.1s;\r\n  text-align: right;\r\n}\r\n\r\n.input:focus {\r\n  border: 1.5px solid var(--color-reset);\r\n  border-radius: 5px;\r\n}\r\n\r\nmain {\r\n  width: 60%;\r\n  padding: 1.5rem 2rem;\r\n  background-color: var(--color-neutral);\r\n  border-radius: 1rem;\r\n  display: grid;\r\n  grid-template-columns: repeat(2, 1fr);\r\n  gap: 1.5rem;\r\n  margin: 5rem;\r\n}\r\n\r\nsection {\r\n  background-color: var(--color-mainbg);\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 100%;\r\n  height: 100vh;\r\n}\r\n\r\n.side--1 {\r\n  /* width: 50%; */\r\n  display: grid;\r\n  grid-template-columns: auto;\r\n  gap: 2.5rem;\r\n  padding: 1rem 0;\r\n  height: 100%;\r\n}\r\n\r\n.side--2 {\r\n  /* width: 50%; */\r\n  background-color: var(--color-buttonandtext);\r\n  padding: 2rem 1.5rem;\r\n  border-radius: 0.5rem;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n\r\n.hidden {\r\n  display: none;\r\n}\r\n\r\n.tips {\r\n  display: grid;\r\n  grid-template-columns: repeat(3, 1fr);\r\n  gap: 0.5rem;\r\n}\r\n\r\n.output {\r\n  display: grid;\r\n  grid-template-columns: auto;\r\n  gap: 1rem;\r\n}\r\n\r\n.outp